<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:tools="http://schemas.android.com/tools"
    tools:context="com.example.enrique.cpr.consciente.consciente">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="0px"
            android:layout_weight="12"
            android:orientation="vertical">

            <Button
                android:layout_width="match_parent"
                android:layout_height="0px"
                android:layout_weight="1"
                android:background="@drawable/custom_button5"
                android:textAllCaps="false"
                android:textSize="25sp"
                android:layout_marginBottom="5sp"
                android:layout_marginLeft="5sp"
                android:layout_marginRight="5sp"
                android:layout_marginTop="5sp"
                android:textColor="@color/colorPrimary"
                android:text="@string/golped"
                android:id="@+id/golpecalor"/>

            <Button
                android:layout_width="match_parent"
                android:layout_height="0px"
                android:layout_weight="1"
                android:background="@drawable/custom_button5"
                android:textAllCaps="false"
                android:textSize="25sp"
                android:layout_marginBottom="5sp"
                android:layout_marginLeft="5sp"
                android:layout_marginRight="5sp"
                android:layout_marginTop="5sp"
                android:textColor="@color/colorPrimary"
                android:text="@string/fuego"
                android:id="@+id/fuego"/>

            <Button
                android:layout_width="match_parent"
                android:layout_height="0px"
                android:layout_weight="1"
                android:background="@drawable/custom_button5"
                android:textAllCaps="false"
                android:textSize="25sp"
                android:layout_marginBottom="5sp"
                android:layout_marginLeft="5sp"
                android:layout_marginRight="5sp"
                android:layout_marginTop="5sp"
                android:textColor="@color/colorPrimary"
                android:text="@string/atragar"
                android:id="@+id/atraga"/>

            <Button
                android:layout_width="match_parent"
                android:layout_height="0px"
                android:background="@drawable/custom_button5"
                android:textAllCaps="false"
                android:textSize="25sp"
                android:layout_marginBottom="5sp"
                android:layout_marginLeft="5sp"
                android:layout_marginRight="5sp"
                android:layout_marginTop="5sp"
                android:textColor="@color/colorPrimary"
                android:layout_weight="1"
                android:text="@string/hemo"
                android:id="@+id/hemo" />

            <Button
                android:layout_width="match_parent"
                android:layout_height="0px"
                android:background="@drawable/custom_button5"
                android:textAllCaps="false"
                android:textSize="25sp"
                android:layout_marginBottom="5sp"
                android:layout_marginLeft="5sp"
                android:layout_marginRight="5sp"
                android:layout_marginTop="5sp"
                android:textColor="@color/colorPrimary"
                android:layout_weight="1"
                android:text="@string/herida"
                android:id="@+id/herida"/>

            <Button
                android:layout_width="match_parent"
                android:layout_height="0px"
                android:layout_weight="1"
                android:background="@drawable/custom_button5"
                android:textAllCaps="false"
                android:textSize="25sp"
                android:layout_marginBottom="5sp"
                android:layout_marginLeft="5sp"
                android:layout_marginRight="5sp"
                android:layout_marginTop="5sp"
                android:textColor="@color/colorPrimary"
                android:text="@string/veneno"
                android:id="@+id/veneno"/>

            <Button
                android:layout_width="match_parent"
                android:layout_height="0px"
                android:layout_weight="1"
                android:text="@string/fractura"
                android:background="@drawable/custom_button5"
                android:textAllCaps="false"
                android:textSize="25sp"
                android:layout_marginBottom="5sp"
                android:layout_marginLeft="5sp"
                android:layout_marginRight="5sp"
                android:layout_marginTop="5sp"
                android:textColor="@color/colorPrimary"
                android:id="@+id/fractura"/>

            <Button
                android:layout_width="match_parent"
                android:layout_height="0px"
                android:layout_weight="1"
                android:background="@drawable/custom_button5"
                android:textAllCaps="false"
                android:textSize="25sp"
                android:layout_marginBottom="5sp"
                android:layout_marginLeft="5sp"
                android:layout_marginRight="5sp"
                android:layout_marginTop="5sp"
                android:textColor="@color/colorPrimary"
                android:text="@string/rayo"
                android:id="@+id/rayo"/>

            <Button
                android:layout_width="match_parent"
                android:layout_height="0px"
                android:background="@drawable/custom_button5"
                android:textAllCaps="false"
                android:textSize="25sp"
                android:layout_marginBottom="5sp"
                android:layout_marginLeft="5sp"
                android:layout_marginRight="5sp"
                android:layout_marginTop="5sp"
                android:textColor="@color/colorPrimary"
                android:layout_weight="1"
                android:text="@string/golpes"
                android:id="@+id/golpes"/>
        </LinearLayout>
    </LinearLayout>
</LinearLayout>