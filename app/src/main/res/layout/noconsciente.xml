<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.example.enrique.cpr.no_consciente.noconsciente">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="0px"
            android:layout_weight="12"
            android:orientation="vertical">

            <Button
                android:id="@+id/a"
                android:layout_width="match_parent"
                android:layout_height="0px"
                android:layout_weight="1"
                android:background="@drawable/custom_button5"
                android:textAllCaps="false"
                android:textSize="25sp"
                android:layout_marginBottom="5sp"
                android:layout_marginLeft="5sp"
                android:layout_marginRight="5sp"
                android:layout_marginTop="5sp"
                android:textColor="@color/colorPrimary"
                android:text="A"/>

            <Button
                android:id="@+id/b"
                android:layout_width="match_parent"
                android:layout_height="0px"
                android:layout_weight="1"
                android:background="@drawable/custom_button5"
                android:textAllCaps="false"
                android:textSize="25sp"
                android:layout_marginBottom="5sp"
                android:layout_marginLeft="5sp"
                android:layout_marginRight="5sp"
                android:layout_marginTop="5sp"
                android:textColor="@color/colorPrimary"
                android:text="B" />

            <Button
                android:id="@+id/c"
                android:layout_width="match_parent"
                android:layout_height="0px"
                android:layout_weight="1"
                android:background="@drawable/custom_button5"
                android:textAllCaps="false"
                android:textSize="25sp"
                android:layout_marginBottom="5sp"
                android:layout_marginLeft="5sp"
                android:layout_marginRight="5sp"
                android:layout_marginTop="5sp"
                android:textColor="@color/colorPrimary"
                android:text="C" />

            <Button
                android:id="@+id/parada"
                android:layout_width="match_parent"
                android:layout_height="0px"
                android:layout_weight="1"
                android:background="@drawable/custom_button5"
                android:textAllCaps="false"
                android:textSize="25sp"
                android:layout_marginBottom="5sp"
                android:layout_marginLeft="5sp"
                android:layout_marginRight="5sp"
                android:layout_marginTop="5sp"
                android:textColor="@color/colorPrimary"
                android:text="@string/paradaC" />

            <Button
                android:id="@+id/atragar"
                android:layout_width="match_parent"
                android:layout_height="0px"
                android:layout_weight="1"
                android:background="@drawable/custom_button5"
                android:textAllCaps="false"
                android:textSize="25sp"
                android:layout_marginBottom="5sp"
                android:layout_marginLeft="5sp"
                android:layout_marginRight="5sp"
                android:layout_marginTop="5sp"
                android:textColor="@color/colorPrimary"
                android:text="@string/atragar"/>

            <Button
                android:id="@+id/covulsion"
                android:layout_width="match_parent"
                android:layout_height="0px"
                android:layout_weight="1"
                android:background="@drawable/custom_button5"
                android:textSize="25sp"
                android:layout_marginBottom="5sp"
                android:layout_marginLeft="5sp"
                android:layout_marginRight="5sp"
                android:layout_marginTop="5sp"
                android:textColor="@color/colorPrimary"
                android:text="@string/convulsion"
                android:textAllCaps="false"/>

            <Button
                android:id="@+id/desmayo"
                android:layout_width="match_parent"
                android:layout_height="0px"
                android:layout_weight="1"
                android:background="@drawable/custom_button5"
                android:textAllCaps="false"
                android:textSize="25sp"
                android:layout_marginBottom="5sp"
                android:layout_marginLeft="5sp"
                android:layout_marginRight="5sp"
                android:layout_marginTop="5sp"
                android:textColor="@color/colorPrimary"
                android:text="@string/desmayo" />

            <Button
                android:id="@+id/coma"
                android:layout_width="match_parent"
                android:layout_height="0px"
                android:layout_weight="1"
                android:background="@drawable/custom_button5"
                android:textAllCaps="false"
                android:textSize="25sp"
                android:layout_marginBottom="5sp"
                android:layout_marginLeft="5sp"
                android:layout_marginRight="5sp"
                android:layout_marginTop="5sp"
                android:textColor="@color/colorPrimary"
                android:text="@string/coma"/>

            <Button
                android:id="@+id/trauma"
                android:layout_width="match_parent"
                android:layout_height="0px"
                android:layout_weight="1"
                android:background="@drawable/custom_button5"
                android:textAllCaps="false"
                android:textSize="25sp"
                android:layout_marginBottom="5sp"
                android:layout_marginLeft="5sp"
                android:layout_marginRight="5sp"
                android:layout_marginTop="5sp"
                android:textColor="@color/colorPrimary"
                android:text="@string/trauma" />

        </LinearLayout>
    </LinearLayout>
</LinearLayout>